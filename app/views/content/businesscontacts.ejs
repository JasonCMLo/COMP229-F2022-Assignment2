<div class="container">
    <h1>Business Contacts</h1>
        <div class="row">
        <div class="col">
    
            <table class="table table-striped table-bordered table-hover">
                <thead class="table-dark">
                    <tr>
                        <th scope="col" class="text-center">#</th>
                        <th scope="col">Name</th>
                        <th scope="col">Phone</th>
                        <th scope="col">Email</th>
                    </tr>
                </thead>
                <tbody id="contactList">
                    <!-- Repeatable Rows -->
                    <% for(let count = 0; count < businesscontacts.length; count++) { %>
                        <tr>
                            <th scope="row" class="text-center"><%= count + 1 %></th>
                            <td><%= businesscontacts[count].conctactname %></td>
                            <td><%= businesscontacts[count].contactnumber %></td>
                            <td><%= businesscontacts[count].contactemail %></td>
                            
                        </tr>
                    <% } %>
                </tbody>
            </table>
        </div>
    </div>
</div>

